{
    "source": {
      "include": ["src"],
      "includePattern": "\\.(jsx|js|ts|tsx)$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown",
      "better-docs/component",
      "better-docs/jsx"
    ],
    "tags": {
      "allowUnknownTags": ["component"],
      "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
      "better-docs": {
        "name": "Your React Components",
        "title": "React Components Documentation",
        "navLinks": [
          {
            "label": "GitHub",
            "href": "https://github.com/jsovelove/civil-rights-history-project"
          },
          {
            "label": "Project Website",
            "href": "https://jsovelove.github.io/civil-rights-history-project/"
          }
        ]
      },
      "cleverLinks": false,
      "monospaceLinks": false,
      "search": true,
      "default": {
        "staticFiles": {
          "include": [
            "./README.md"
          ]
        }
      }
    },
    "opts": {
      "destination": "docs",
      "recurse": true,
      "readme": "README.md",
      "template": "node_modules/better-docs"
    }
  }